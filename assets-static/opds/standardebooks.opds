	<title>McTeague</title>
			<author>
			<name>Frank Norris</name>
			<uri>https://en.wikipedia.org/wiki/Frank_Norris</uri>			<schema:alternateName>Benjamin Franklin Norris Jr.</schema:alternateName>			<schema:sameAs>https://id.loc.gov/authorities/names/n80032817</schema:sameAs>		</author>
