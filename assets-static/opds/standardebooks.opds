	<title>Mr. Standfast</title>
			<author>
			<name>John Buchan</name>
			<uri>https://en.wikipedia.org/wiki/John_Buchan</uri>						<schema:sameAs>http://id.loc.gov/authorities/names/n79045167</schema:sameAs>		</author>
		<dc:issued>2019-09-18T21:53:08Z</dc:issued>
	<updated>2021-02-23T20:04:40Z</updated>
	<dc:language>en-GB</dc:language>
	<dc:publisher>Standard Ebooks</dc:publisher>
