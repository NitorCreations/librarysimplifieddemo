	<title>The Enchanted Castle</title>
			<author>
			<name>E. Nesbit</name>
			<uri>https://en.wikipedia.org/wiki/E._Nesbit</uri>			<schema:alternateName>Edith Nesbit</schema:alternateName>			<schema:sameAs>http://id.loc.gov/authorities/names/n85097242</schema:sameAs>		</author>
		<dc:issued>2018-04-02T03:44:34Z</dc:issued>
	<updated>2021-03-03T16:54:08Z</updated>
	<dc:language>en-GB</dc:language>
	<dc:publisher>Standard Ebooks</dc:publisher>
