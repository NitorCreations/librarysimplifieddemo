	<title>Middlemarch</title>
			<author>
			<name>George Eliot</name>
			<uri>https://en.wikipedia.org/wiki/George_Eliot</uri>						<schema:sameAs>https://id.loc.gov/authorities/names/n79045512</schema:sameAs>		</author>
