	<title>Hard Times</title>
			<author>
			<name>Charles Dickens</name>
			<uri>https://en.wikipedia.org/wiki/Charles_Dickens</uri>			<schema:alternateName>Charles John Huffam Dickens</schema:alternateName>			<schema:sameAs>http://id.loc.gov/authorities/names/n78087607</schema:sameAs>		</author>
		<dc:issued>2018-10-02T00:05:33Z</dc:issued>
	<updated>2021-02-23T19:57:44Z</updated>
	<dc:language>en-GB</dc:language>
	<dc:publisher>Standard Ebooks</dc:publisher>
		<dc:source>https://www.gutenberg.org/ebooks/786</dc:source>
