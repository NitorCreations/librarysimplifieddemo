			<author>
			<name>Ivan Turgenev</name>
			<uri>https://en.wikipedia.org/wiki/Ivan_Turgenev</uri>			<schema:alternateName>Ivan Sergeyevich Turgenev</schema:alternateName>			<schema:sameAs>https://id.loc.gov/authorities/names/n79144981</schema:sameAs>		</author>
