	<title>Captain Blood</title>
			<author>
			<name>Rafael Sabatini</name>
			<uri>https://en.wikipedia.org/wiki/Rafael_Sabatini</uri>						<schema:sameAs>http://id.loc.gov/authorities/names/n50016255</schema:sameAs>		</author>
		<dc:issued>2017-11-09T23:25:30Z</dc:issued>
	<updated>2021-02-23T20:08:50Z</updated>
	<dc:language>en-GB</dc:language>
	<dc:publisher>Standard Ebooks</dc:publisher>
