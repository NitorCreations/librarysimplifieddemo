	<title>Frankenstein</title>
			<author>
			<name>Mary Shelley</name>
			<uri>https://en.wikipedia.org/wiki/Mary_Shelley</uri>			<schema:alternateName>Mary Wollstonecraft Shelley</schema:alternateName>			<schema:sameAs>http://id.loc.gov/authorities/names/n79061063</schema:sameAs>		</author>
		<dc:issued>2015-05-12T00:01:00Z</dc:issued>
	<updated>2021-02-23T20:07:07Z</updated>
	<dc:language>en-GB</dc:language>
	<dc:publisher>Standard Ebooks</dc:publisher>
