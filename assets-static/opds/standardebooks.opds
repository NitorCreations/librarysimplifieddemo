	<title>The Jungle</title>
			<author>
			<name>Upton Sinclair</name>
			<uri>https://en.wikipedia.org/wiki/Upton_Sinclair</uri>						<schema:sameAs>http://id.loc.gov/authorities/names/n79127862</schema:sameAs>		</author>
		<dc:issued>2014-05-25T00:00:00Z</dc:issued>
