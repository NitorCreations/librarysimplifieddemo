	<title>Ivanhoe</title>
			<author>
			<name>Walter Scott</name>
			<uri>https://en.wikipedia.org/wiki/Walter_Scott</uri>						<schema:sameAs>http://id.loc.gov/authorities/names/n78095541</schema:sameAs>		</author>
		<dc:issued>2018-04-30T02:01:19Z</dc:issued>
	<updated>2021-02-23T20:11:47Z</updated>
	<dc:language>en-GB</dc:language>
