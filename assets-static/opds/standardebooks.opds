			<author>
			<name>Evelyn Underhill</name>
			<uri>https://en.wikipedia.org/wiki/Evelyn_Underhill</uri>						<schema:sameAs>http://id.loc.gov/authorities/names/n79026720</schema:sameAs>		</author>
