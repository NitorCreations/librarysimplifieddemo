	<title>Emma</title>
			<author>
			<name>Jane Austen</name>
			<uri>https://en.wikipedia.org/wiki/Jane_Austen</uri>						<schema:sameAs>http://id.loc.gov/authorities/names/n79032879</schema:sameAs>		</author>
		<dc:issued>2018-03-21T00:17:13Z</dc:issued>
