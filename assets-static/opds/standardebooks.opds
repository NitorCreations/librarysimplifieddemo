	<title>Nicomachean Ethics</title>
			<author>
			<name>Aristotle</name>
			<uri>https://en.wikipedia.org/wiki/Aristotle</uri>						<schema:sameAs>http://id.loc.gov/authorities/names/n79004182</schema:sameAs>		</author>
		<dc:issued>2018-12-03T20:14:08Z</dc:issued>
	<updated>2021-02-23T19:56:13Z</updated>
	<dc:language>en-GB</dc:language>
