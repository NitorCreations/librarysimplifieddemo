			<author>
			<name>Hermann Hesse</name>
			<uri>https://en.wikipedia.org/wiki/Hermann_Hesse</uri>			<schema:alternateName>Hermann Karl Hesse</schema:alternateName>			<schema:sameAs>http://id.loc.gov/authorities/names/n79006698</schema:sameAs>		</author>
